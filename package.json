    "./netease/song.ts": "./netease/song.ts"
  },
  "scripts": {
    "lint": "biome check --write . && prettier . --write"
  },
  "dependencies": {
    "aplayer-ts": "^2.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-astro-organize-imports": "^0.4.11",
    "prettier-plugin-organize-imports": "^4.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.10.5",
    "astro": "^5.1.3"
